{
  "root": true,
  "project": {
    "name": "GWS Blog",
    "basePath": "/studio"
  },
  "api": {
    "projectId": "0hm5a81q",
    "dataset": "production"
  },
  "plugins": [
    "@sanity/base",
    "@sanity/default-layout",
    "@sanity/default-login",
    "@sanity/desk-tool",
    "@sanity/vision",
    "asset-source-unsplash",
    "media",
    "seo-tools",
    "markdown",
    "social-preview",
    "asset-source-pexels",
    "bynder-input",
    "@andre-brdoch/sanity-plugin-schema-inspector",
    "position-input",
    "icon-picker",
    "@sanity/code-input",
    "@sanity/dashboard"
  ],
  "env": {
    "development": {
      "plugins": [
        "@sanity/vision"
      ]
    }
  },
  "parts": [
    {
      "name": "part:@sanity/base/schema",
      "path": "./schemas/schema"
    },
    {
      "implements": "part:@sanity/base/theme/variables/override-style",
      "path": "./static/style.css"
    },
    { 
      "implements": "part:@sanity/dashboard/config", 
      "path": "./dashboardConfig.js" 
    }
  ]
}
